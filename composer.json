    "description": "speed up lumen with swoole",
    "type": "library",
    "license": "MIT",
    "version": "1.2.0",
    "authors": [
        {
            "name": "Breeze",
            "email": "805511939@qq.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "ext-pcntl" : "*",
        "ext-posix" : "*",
        "ext-swoole" : ">=1.9.2",
        "laravel/lumen-framework":">=5.5.0",
        "illuminate/redis": ">=5.5.0"
